#Print multiplication table up to times 7 for 1-7
for i in range(1, 7):
              for j in range(1, 7):
                  print(i, j, i*j)


fruits = ["apples", "oranges", "mangoes"]

#Print the number of letters in string
for fruit in fruits:
              string_size = 0
              for alphabet in fruit:
                  string_size += 1
              print("name of fruit: ", fruit, "has ", string_size, "letters")
